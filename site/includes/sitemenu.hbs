<div class="container nvbar">
	<h2>{{{i18n "tmpl-topics-menu"}}}</h2>
    <div class="row">
        <ul class="list-inline menu" role="menu">
{{#unless page.altMenu}}
				
			<li><a href="/geonetwork/home/{{#isnt language "fr"}}eng{{else}}fre{{/isnt}}" title="{{t "page.menu.homeTitle"}}" tabindex="0" aria-posinset="1" aria-setsize="6" role="menuitem">{{t "page.menu.homeText"}}</a></li>
            <li><a href="/geonetwork/search/{{#isnt language "fr"}}eng{{else}}fre{{/isnt}}" title="{{t "page.menu.searchTitle"}}" class="item" tabindex="-1" aria-posinset="2" aria-setsize="6" role="menuitem" aria-haspopup="true">{{t "page.menu.searchText"}}</a></li>
            <li><a href="/geonetwork/browse/{{#isnt language "fr"}}eng{{else}}fre{{/isnt}}" title="{{t "page.menu.browseTitle"}}" tabindex="-1" aria-posinset="3" aria-setsize="6" role="menuitem">{{t "page.menu.browseText"}}</a></li>
